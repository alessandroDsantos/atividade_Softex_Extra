import express from 'express';

const app = express();
const port = 8080;

app.get('/', function(req, res) {
  res.send(`GET->Hello World!`);
});

app.post('/post', function (req, res) {
    res.send(`POST->Hello World!`);
  });

app.listen(port, () => {
    console.log(`Exemplo app node rodando no endere√ßo http://localhost:${port}`)
 });
